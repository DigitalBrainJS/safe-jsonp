    "watch": "gulp watch",
    "dev": "gulp dev",
    "prepublish": "npm run build"
  },
  "license": "MIT",
  "homepage": "https://github.com/DigitalBrainJS/safe-jsonp",
  "bugs": "https://github.com/DigitalBrainJS/safe-jsonp/issues",
  "repository": {
    "type": "git",
