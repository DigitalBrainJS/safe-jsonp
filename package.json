    "safe",
    "fetch",
    "get",
    "load",
    "ajax",
    "http",
    "sandbox"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "connect": "^3.6.6",
    "expect": "^23.6.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-better-rollup": "^3.3.0",
    "gulp-plumber": "^1.2.0",
    "mocha": "^5.2.0",
    "rollup-plugin-commonjs": "^9.1.4",
    "rollup-plugin-node-resolve": "^3.4.0",
    "run-sequence": "^2.2.1",
    "serve-static": "^1.13.2"
  },
  "testling": {
    "harness": "mocha-bdd",
